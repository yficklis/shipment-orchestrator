    "components": {
        "schemas": {
            "Shipment": {
                "title": "Shipment",
                "description": "Shipment model representing a USPS shipping label",
                "required": [
                    "id",
                    "user_id",
                    "tracking_code",
                    "carrier",
                    "status"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "user_id": {
                        "type": "integer",
                        "example": 1
                    },
                    "tracking_code": {
                        "type": "string",
                        "example": "9400111899562537624127"
                    },
                    "carrier": {
                        "type": "string",
                        "example": "USPS"
                    },
                    "easypost_shipment_id": {
                        "type": "string",
                        "example": "shp_abc123"
                    },
                    "status": {
                        "type": "string",
                        "enum": [
                            "created",
                            "purchased",
                            "voided"
                        ],
                        "example": "purchased"
                    },
                    "from_name": {
                        "type": "string",
                        "example": "John Doe"
                    },
                    "from_street1": {
                        "type": "string",
                        "example": "123 Main St"
                    },
                    "from_street2": {
                        "type": "string",
                        "nullable": true
                    },
                    "from_city": {
                        "type": "string",
                        "example": "San Francisco"
                    },
                    "from_state": {
                        "type": "string",
                        "example": "CA"
                    },
                    "from_zip": {
                        "type": "string",
                        "example": "94105"
                    },
                    "from_country": {
                        "type": "string",
                        "example": "US"
                    },
                    "from_phone": {
                        "type": "string",
                        "nullable": true
                    },
                    "from_email": {
                        "type": "string",
                        "nullable": true
                    },
                    "to_name": {
                        "type": "string",
                        "example": "Jane Smith"
                    },
                    "to_street1": {
                        "type": "string",
                        "example": "456 Oak Ave"
                    },
                    "to_street2": {
                        "type": "string",
                        "nullable": true
                    },
                    "to_city": {
                        "type": "string",
                        "example": "New York"
                    },
                    "to_state": {
                        "type": "string",
                        "example": "NY"
                    },
                    "to_zip": {
                        "type": "string",
                        "example": "10001"
                    },
                    "to_country": {
                        "type": "string",
                        "example": "US"
                    },
                    "to_phone": {
                        "type": "string",
                        "nullable": true
                    },
                    "to_email": {
                        "type": "string",
                        "nullable": true
                    },
                    "weight": {
                        "description": "Weight in ounces",
                        "type": "number",
                        "format": "float",
                        "example": 16
                    },
                    "length": {
                        "description": "Length in inches",
                        "type": "number",
                        "format": "float",
                        "nullable": true
                    },
                    "width": {
                        "description": "Width in inches",
                        "type": "number",
                        "format": "float",
                        "nullable": true
                    },
                    "height": {
                        "description": "Height in inches",
                        "type": "number",
                        "format": "float",
                        "nullable": true
                    },
                    "label_url": {
                        "type": "string",
                        "format": "url",
                        "example": "https://easypost-files.s3.amazonaws.com/..."
                    },
                    "tracking_url": {
                        "type": "string",
                        "format": "url",
                        "nullable": true
                    },
                    "postage_label_url": {
                        "type": "string",
                        "format": "url",
                        "nullable": true
                    },
                    "rate_amount": {
                        "type": "number",
                        "format": "float",
                        "example": 7.33
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "deleted_at": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    }
                },
                "type": "object"
            }
